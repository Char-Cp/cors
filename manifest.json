{
    "manifest_version": 3,
    "name": "BINANCE-CORS",
    "version": "2.0",
    "declarative_net_request": {
        "rule_resources": [
            {
                "id": "binance",
                "enabled": true,
                "path": "binance.json"
            },{
                "id": "gate",
                "enabled": true,
                "path": "gate.json"
            }
        ]
    },
    "permissions": [
        "declarativeNetRequest",
        "declarativeNetRequestWithHostAccess",
        "activeTab"
    ],
    "host_permissions": [
        "*://*.binance.com/*",
        "*://*.gateio.ws/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": "close.png"
    }
}