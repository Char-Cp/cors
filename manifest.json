{
  "manifest_version": 2,
  "name": "BINANCE-CORS",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": {
    "45": "close.png"
  }
}
}
